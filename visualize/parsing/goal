digraph {
	graph [label=Goal labelloc=top]
	node [shape=box]
	H [label="H:Module"]
	S [label="S:Module"]
	V [label="V:Module"]
	O [label="O:Module"]
	T [label="T:Module"]
	noun [label="noun:Noun"]
	verb [label="verb:Verb"]
	head [label="head:Head"]
	tail [label="tail:Tail"]
	H -> S [label="next to"]
	S -> V [label="next to"]
	V -> O [label="next to"]
	O -> T [label="next to"]
	H -> head [label=is]
	S -> noun [label=is]
	V -> verb [label=is]
	O -> noun [label=is]
	T -> tail [label=is]
}
